CREATE TABLE `coverage` (
  `idcoverage` int(11) NOT NULL AUTO_INCREMENT,
  `CoverageZip` varchar(6) DEFAULT NULL,
  `OfficeZip` varchar(6) DEFAULT NULL,
  PRIMARY KEY (`idcoverage`)
) ENGINE=InnoDB AUTO_INCREMENT=152 DEFAULT CHARSET=utf8;


CREATE TABLE `customers` (
  `idcustomers` int(11) NOT NULL AUTO_INCREMENT,
  `CustomerName` varchar(45) DEFAULT NULL,
  `Address` varchar(500) DEFAULT NULL,
  `MobileNumber` varchar(45) DEFAULT NULL,
  `Email` varchar(150) DEFAULT NULL,
  `City` varchar(45) DEFAULT NULL,
  `State` varchar(45) DEFAULT NULL,
  `ZipCode` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`idcustomers`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


CREATE TABLE `orders` (
  `idOrders` int(11) NOT NULL AUTO_INCREMENT,
  `MobileNumber` varchar(15) DEFAULT NULL,
  `ZipCode` varchar(6) DEFAULT NULL,
  `EmailAddress` varchar(100) DEFAULT NULL,
  `rating` int(11) DEFAULT NULL,
  PRIMARY KEY (`idOrders`)
) ENGINE=InnoDB AUTO_INCREMENT=44 DEFAULT CHARSET=utf8;

CREATE TABLE `repair` (
  `idrepair` int(11) NOT NULL AUTO_INCREMENT,
  `CustomerID` int(11) DEFAULT NULL,
  `repairdetail` varchar(500) DEFAULT NULL,
  `rating` int(11) DEFAULT NULL,
  PRIMARY KEY (`idrepair`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `schedule` (
  `idschedule` int(11) NOT NULL AUTO_INCREMENT,
  `order_repair_id` int(11) DEFAULT NULL,
  `technician_id` int(11) DEFAULT NULL,
  `ynconfirmed` varchar(1) DEFAULT NULL,
  `sdate` datetime DEFAULT NULL,
  `slotnumber` varchar(45) DEFAULT NULL,
  `stype` varchar(45) DEFAULT NULL,
  `yncompleted` varchar(1) DEFAULT NULL,
  PRIMARY KEY (`idschedule`)
) ENGINE=InnoDB AUTO_INCREMENT=14 DEFAULT CHARSET=utf8;

CREATE TABLE `technician` (
  `idTechnician` int(11) NOT NULL AUTO_INCREMENT,
  `TechnicianEmpID` varchar(45) DEFAULT NULL,
  `TechnicianName` varchar(45) DEFAULT NULL,
  `OfficeZip` varchar(6) DEFAULT NULL,
  `EmailID` varchar(100) DEFAULT NULL,
  `Phone` varchar(45) DEFAULT NULL,
  `SkillSet` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`idTechnician`)
) ENGINE=InnoDB AUTO_INCREMENT=43 DEFAULT CHARSET=utf8;


